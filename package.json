  "bugs": {
    "url": "https://github.com/xhc2008/CABM/issues"
  },
  "homepage": "https://github.com/xhc2008/CABM#readme",
  "devDependencies": {
    "electron": "^37.2.6"
  },
  "dependencies": {
    "concurrently": "^9.2.0",
    "wait-on": "^8.0.4"
  }
}
